---
import PageWithSidebar from "../layouts/PageWithSidebar.astro";
---

<PageWithSidebar title="Page Not Found">
  <article class="post">
    <header>
      <div class="title">
        <h2>404 - Page Not Found</h2>
        <p>The page you're looking for doesn't seem to exist.</p>
      </div>
    </header>
    <footer>
      <ul class="actions">
        <li><a href="/" class="button big">Go Home</a></li>
      </ul>
    </footer>
  </article>
</PageWithSidebar>
